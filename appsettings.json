{
  "Cache": {
    "CachePath": "D:\\PhotoCache",
    "MaxMemoryCacheSizeMB": 500,
    "ThumbnailSize": 512,
    "JpegQuality": 85,
    "MaxParallelGenerations": 4,
    "EnableRAWSupport": true
  },
  "Performance": {
    "MaxParallelLoads": 6,
    "PrefetchCount": 20
  },
  "Export": {
    "DefaultTemplate": "instagram-square",
    "OutputPath": "C:\\Users\\{username}\\Pictures\\Exports"
  },
  "UI": {
    "GridThumbnailSize": 256,
    "EnableGPUAcceleration": true,
    "ArrowKeyNavigationMode": "GridFocus"
  },
  "Database": {
    "ConnectionString": "Data Source=photos.db"
  },
  "DatabaseMode": {
    "DefaultScanRecursive": true,
    "AutoScanOnStartup": false,
    "ScanIntervalDays": 7
  },
  "FolderMode": {
    "SessionSaveLocation": "Temp",
    "AutoSaveIntervalSeconds": 60
  }
}
